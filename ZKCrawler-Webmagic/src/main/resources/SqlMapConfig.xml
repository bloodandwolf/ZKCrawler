<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

<environments default = "development">
    <environment id = "development">
        <transactionManager type = "JDBC"/>
        <dataSource type = "POOLED">
            <property name = "driver" value = "com.mysql.jdbc.Driver"/>
            <property name = "url" value = "jdbc:mysql://127.0.0.1:3306/datastory?useUnicode=true&amp;characterEncoding=UTF-8"/>
            <property name = "username" value = "root"/>
            <property name = "password" value = "builder"/>
        </dataSource>
    </environment>
</environments>
    <mappers>
        <mapper class="com.gy.wm.mapper.CrawlDataMapper"></mapper>
        <mapper class="com.gy.wm.mapper.ParserMapper"></mapper>
        <mapper class="com.gy.wm.mapper.TaskConfigMapper"></mapper>
        <mapper class="com.gy.wm.mapper.WxArticleMapper"></mapper>
    </mappers>
</configuration>